{
  "name": "YouTube Auto Add Skipper Pro",
  "short_name": "YouTube Ad Auto-skipper",
  "description": "Based on work by squgeim, youtube ad skipper but with extra features",
  "manifest_version": 2,
  "version": "0.5.1",
  "author": "Shreya Dahal, Abdul Rauf",
  "homepage_url": "https://github.com/rauf-source/yt-ad-autoskipper",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "skipad.js",
        "mute_player.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "icons": {
    "128": "logo.png"
  }
}